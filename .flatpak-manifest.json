{
    "id": "org.kde.isoimagewriter",
    "branch": "master",
    "runtime": "org.kde.Platform",
    "runtime-version": "5.15-22.08",
    "sdk": "org.kde.Sdk",
    "command": "isoimagewriter",
    "tags": [
        "nightly"
    ],
